    <bpmn:userTask id="Task_08tfpn1" name="Odabir recezeanta" camunda:assignee="${odabrani_urednik}">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="recezent" label="Odabir recezenta" type="enum" />
          <camunda:formField id="zavrsi_odabir" label="Zavrsi odabir" type="boolean" />
        </camunda:formData>
        <camunda:taskListener event="complete">
          <camunda:script scriptFormat="Groovy">taskFormData = task.getExecution().getProcessEngineServices().getFormService().getTaskFormData(task.getId());

listaRecezenata = task.getExecution().getVariable("listaRecezenata")
if (listaRecezenata==null)
    listaRecezenata = new java.util.ArrayList();

formFields = taskFormData.getFormFields();
if(formFields!=null){
   for(field in formFields){
       if(field.getId() == "recezent"){
           if(field.getValue()!="none")
                listaRecezenata.add(field.getValue());
       }
   }
}

task.getExecution().setVariable("listaRecezenata", listaRecezenata);

atLeastTwo = listaRecezenata.size() &gt;= 2
task.getExecution().setVariable("atLeastTwo", atLeastTwo);</camunda:script>
        </camunda:taskListener>
        <camunda:taskListener event="create">
          <camunda:script scriptFormat="Groovy">taskFormData = task.getExecution().getProcessEngineServices().getFormService().getTaskFormData(task.getId());
accountants = task.getExecution().getVariable("casopis_recezenti");

formFields = taskFormData.getFormFields();
if(formFields!=null){
   for(field in formFields){
       if( field.getId() == "recezent" ){
           //ovo je nase select polje
           items = field.getType().getValues();
           for(user in accountants){
               items.put(user.getId(),user.getFirstName()+" "+user.getLastName());
           }
       }
   }
}</camunda:script>
        </camunda:taskListener>
